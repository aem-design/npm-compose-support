{
  "author": "Chris Shaw <me@cshaw.xyz>",
  "description": "Support code for the compose project",
  "engines": {
    "node": ">= 10.x",
    "yarn": ">= 1.10.x"
  },
  "files": [
    "dist/",
    "types/*.d.ts"
  ],
  "keywords": [
    "aem",
    "design",
    "support",
    "compose",
    "node",
    "logging",
    "log"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "name": "@aem-design/compose-support",
  "repository": {
    "type": "git",
    "url": "https://github.com/aem-design/npm-compose-support"
  },
  "bugs": {
    "url": "https://github.com/aem-design/npm-compose-support/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run clean && npm run build.compile",
    "build.compile": "cross-env NODE_ENV=production tsc",
    "clean": "rm -rf dist",
    "lint": "eslint '**/*.ts'",
    "postversion": "npm run test && git push && git push --tags",
    "watch": "tsc --watch"
  },
  "husky": {
    "skipCI": true,
    "hooks": {
      "pre-commit": "npm run build"
    }
  },
  "version": "1.0.1",
  "dependencies": {
    "chalk": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^12.12.3",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "cross-env": "^7.0.0",
    "eslint": "^5.16.0",
    "husky": "^3.0.9",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
